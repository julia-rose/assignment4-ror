<h1>This is users#index</h1>

<!-- Ify ou put this in you layouts/application.html.erb then it will work with any resource -->
<% if flash[:alert] %>
  <p class='alert'><%=h flash[:error] %></p>
<% end -%>
<% if flash[:notice] %>
  <p class='notice'><%=h flash[:notice] %></p>
<% end -%>

<%= @users.each do |user| %>
	<%= user.username %>
	<br>
<% end %>

